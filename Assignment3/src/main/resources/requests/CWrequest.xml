<?xml version="1.0" encoding="UTF-8"?>
<Request 
    xmlns="urn:oasis:names:tc:xacml:2.0:context:schema:os" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation=" urn:oasis:names:tc:xacml:2.0:context:schema:os 
      http://docs.oasis-open.org/xacml/access_control-xacml-2.0-context-schema-os.xsd">
  <Subject>
 
    <Attribute AttributeId="cd-history" DataType="http://www.w3.org/2001/XMLSchema#string"><AttributeValue>cd:rema</AttributeValue></Attribute>    
    <Attribute AttributeId="coi-history" DataType="http://www.w3.org/2001/XMLSchema#string"><AttributeValue>coi:food</AttributeValue></Attribute>
    
    

    <Attribute AttributeId="clean-coi" DataType="http://www.w3.org/2001/XMLSchema#string"><AttributeValue>coi:telecom</AttributeValue></Attribute>
    <Attribute AttributeId="clean-coi" DataType="http://www.w3.org/2001/XMLSchema#string"><AttributeValue>coi:energy</AttributeValue></Attribute>
    

  </Subject>
  <Resource>
    <Attribute AttributeId="urn:oasis:names:tc:xacml:1.0:resource:resource-id" 
        DataType="http://www.w3.org/2001/XMLSchema#string">
      <AttributeValue>randomfilename</AttributeValue>
    </Attribute>
    <Attribute AttributeId="cd"
        DataType="http://www.w3.org/2001/XMLSchema#string">
      <AttributeValue>cd:telenor</AttributeValue>
    </Attribute>
    <Attribute AttributeId="coi"
        DataType="http://www.w3.org/2001/XMLSchema#string">
      <AttributeValue>coi:telecom</AttributeValue>
    </Attribute>
  </Resource>
  <Action>
    <Attribute AttributeId="urn:oasis:names:tc:xacml:1.0:action:action-id"
        DataType="http://www.w3.org/2001/XMLSchema#string">
      <AttributeValue>read</AttributeValue>
    </Attribute>
  </Action>

</Request>
